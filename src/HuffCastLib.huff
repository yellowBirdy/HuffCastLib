#define error Overflow()


/* Methods */
#define macro TO_UINT8() = takes (1) returns (1) {  // [value]
    0x100            // [0xff, value]
    dup2             // [value, 0xff, value]
    lt                // [0/1, value]
    success jumpi     // [ value]
    __ERROR(Overflow)  // [Overflow selector, value]
    0x00 mstore
    0x04 0x00 revert
    success:
}

